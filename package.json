{
  "name": "it-tools-frontend",
  "productName": "IT-Tools",
  "version": "0.0.1",
  "private": true,
  "description": "This repository contains front-end part of IT-Tools project",
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "browser:support": "browserslist --stats && echo '' && browserslist --coverage",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint:styles": "yarn lint:styles:css && yarn lint:styles:js",
    "lint:styles:css": "stylelint \"src/**/*.css\" --config .stylelintrc.css.json",
    "lint:styles:js": "stylelint \"src/**/*.{js,jsx,ts,tsx}\" --config .stylelintrc.js.json",
    "prettier": "prettier",
    "start": "react-scripts start",
    "test": "react-scripts test",
    "test:debug": "react-scripts --inspect-brk test --runInBand --no-cache",
    "tsc": "tsc"
  },
  "dependencies": {
    "@reach/router": "^1.3.3",
    "@testing-library/dom": "^6.15.0",
    "@testing-library/jest-dom": "^5.1.1",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^10.0.0",
    "jest-styled-components": "^7.0.0",
    "mobx": "^5.15.4",
    "mobx-react-lite": "^1.5.2",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-scripts": "3.4.0",
    "styled-components": "^5.0.1",
    "styled-normalize": "^8.0.7",
    "typescript": "~3.8.3"
  },
  "devDependencies": {
    "@types/browserslist": "4.8.0",
    "@types/eslint": "6.1.8",
    "@types/jest": "^24.9.1",
    "@types/node": "^12.12.29",
    "@types/reach__router": "^1.3.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.5",
    "@types/styled-components": "^5.0.1",
    "@typescript-eslint/eslint-plugin": "^2.23.0",
    "@typescript-eslint/parser": "^2.23.0",
    "browserslist": "^4.9.1",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-typescript": "^7.0.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-formatter-pretty": "^3.0.1",
    "eslint-plugin-compat": "^3.5.1",
    "eslint-plugin-eslint-comments": "^3.1.2",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^2.5.0",
    "eslint-plugin-unicorn": "^17.1.0",
    "husky": "^4.2.3",
    "lint-staged": "^10.0.8",
    "prettier": "^1.19.1",
    "source-map-explorer": "^2.3.1",
    "stylelint": "^13.2.1",
    "stylelint-a11y": "^1.2.3",
    "stylelint-config-prettier": "^8.0.1",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-high-performance-animation": "^1.5.0",
    "stylelint-no-unsupported-browser-features": "^4.0.0",
    "stylelint-order": "^4.0.0",
    "stylelint-processor-styled-components": "^1.10.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://git.rit/rit-open-source/it-tools-frontend"
  },
  "engines": {
    "node": ">= 12.16.1",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.22.4"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
